    "gulp-ng-annotate": "~0.3.0",
    "gulp-rimraf": "~0.1.0",
    "gulp-sourcemaps": "^1.1.1",
    "gulp-uglify": "~0.3.1",
    "http-server": "^0.6.1",
    "jasmine-given": "~2.6.2",
    "karma": "~0.12.21",
    "karma-chrome-launcher": "~0.1.3",
    "karma-firefox-launcher": "~0.1.3",
    "karma-jasmine": "~0.2.0",
    "protractor": "~1.0.0"
  }
